{
  "name": "react_template",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "prettier . --write",
    "lint": "eslint .",
    "prepare": "husky",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "duplication-check": "jscpd --pattern \"src/**/*\"",
    "test:coverage-report": "vitest run --coverage",
    "depcheck": "depcheck",
    "vulnerability_retirejs": "retire --ignorefile .retireignore.json",
    "vulnerability_auditjs": "auditjs ossi --quiet --whitelist ./auditjs.json"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.10",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.0",
    "@module-federation/vite": "^1.4.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.2.4",
    "auditjs": "^4.0.46",
    "depcheck": "^1.4.7",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest-axe": "^10.0.0",
    "jest-transform-stub": "^2.0.0",
    "jscpd": "^4.0.5",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "retire": "^5.2.7",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
